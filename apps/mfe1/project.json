{
  "name": "mfe1",
  "sourceRoot": "apps/mfe1/src",
  "projectType": "application",
  "targets": {
    "scan-metadata": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run metadata:scan --moduleFolder=apps/mfe1 --outFile=apps/mfe1/public/manifest.json"
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "mfe1:build",
        "port": 3001,
        "webpackConfig": "apps/mfe1/webpack.config.js",
        "liveReload": true
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "options": {
        "webpackConfig": "apps/mfe1/webpack.config.js",
        "outputPath": "dist/apps/mfe1"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "build-with-metadata": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run mfe1:scan-metadata",
          "nx run mfe1:build"
        ],
        "parallel": false
      }
    }
  }
}
