{
  "name": "metadata",
  "sourceRoot": "tools/metadata/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tools/metadata/tsconfig.lib.json"
      }
    },
    "scan": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx tools/metadata/src/build-metadata.ts --moduleFolder={args.moduleFolder} --outFile={args.outFile}"
      }
    }
  },
  "tags": ["type:tool"]
}
